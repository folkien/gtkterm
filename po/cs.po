#: src/cmdline.c:41
msgid ""
"\n"
"Command line options\n"
msgstr ""
"\n"
"Nastavení příkazové řádky\n"

#: src/cmdline.c:42
msgid "--help or -h : this help screen\n"
msgstr "--help nebo -h : zobrazí tuto nápovědu\n"

#: src/cmdline.c:43
msgid "--config <configuration> or -c : load configuration\n"
msgstr "--config <nastavení> nebo -c : načte nastavení programu uložené v"
"souboru \"nastaveni\"\n"

#: src/cmdline.c:44
msgid "--port <device> or -p : serial port device (default /dev/ttyS0)\n"
msgstr "--port <zařízení> nebo -p : sériový port (výchozí je /dev/ttyS0)\n"

#: src/cmdline.c:45
msgid "--speed <speed> or -s : serial port speed (default 9600)\n"
msgstr "--speed <rychlost> nebo -s : nastavení přenosové rychlosti (výchozí je"
"9600)\n"

#: src/cmdline.c:46
msgid "--bits <bits> or -b : number of bits (default 8)\n"
msgstr "--bits <počet_bitů> nebo -b : počet bitů v jednom rámci (výchozí je"
"8 bitů))\n"

#: src/cmdline.c:47
msgid "--stopbits <stopbits> or -t : number of stopbits (default 1)\n"
msgstr "--stopbits <počet_stop_bitů> nebo -t : počet stopbitů (výchozí je 1)\n"

#: src/cmdline.c:48
msgid "--parity <odd | even> or -a : partity (default none)\n"
msgstr "--parity <odd | even> nebo -a : zabezpečení paritou: lichá (odd),"
"sudá (even) (výchozí je bez paritního bitu)\n"

#: src/cmdline.c:49
msgid "--flow <Xon | CTS> or -w : flow control (default none)\n"
msgstr "--flow <Xon | CTS> nebo -w : ovládání toku dat (výchozí je vypnuto)\n"

#: src/cmdline.c:50
msgid "--delay <ms> or -d : end of line delay in ms (default none)\n"
msgstr "--delay <ms> nebo -d : zpoždění pro znak \"konec řádku\" (výchozí je"
"vypnuto)\n"

#: src/cmdline.c:51
msgid ""
"--char <char> or -r : wait for a special char at end of line (default none)\n"
msgstr ""
"--char <char> nebo -r : čeká na speciální znak na konci řádku (výchozí je"
"vypnuto)\n"

#: src/cmdline.c:52
msgid "--file <filename> or -f : default file to send (default none)\n"
msgstr ""
"--file <nazev_souboru> nebo -f : odešle soubor jako data (výchozí je"
"vypnuto)\n"

#: src/cmdline.c:53
msgid "--echo or -e : switch on local echo\n"
msgstr "--echo nebo -e : zapne echo na místním terminálu\n"

#: src/cmdline.c:144
msgid "Misunderstood command line option\n"
msgstr "Nesprávný argument\n"

#: src/config.c:173
msgid ""
"No valid serial device found in /dev, sorry !\n"
"You should have at least one of these :\n"
"/dev/ttyS*\n"
"/dev/tts/*\n"
"/dev/ttyUSB*\n"
"/dev/usb/tts/*\n"
msgstr ""
"Nenalezeno žádné vhodné zařízení v adresáři /dev, sorry !\n"
"Pro správnou funkci by jste měli mít jedno z těchto zařízení :\n"
"/dev/ttyS*\n"
"/dev/tts/*\n"
"/dev/ttyUSB*\n"
"/dev/usb/tts/*\n"

